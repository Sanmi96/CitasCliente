<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.sanmi.citasClientes.hibernate.bean.Cliente" table="CLIENTE">
  <id column="ID" name="id" type="int">
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="nombre">
   <column name="NOMBRE"/>
  </property>
  <property generated="never" lazy="false" name="primer_apellido">
   <column name="PRIMER_APELLIDO"/>
  </property>
  <property generated="never" lazy="false" name="segundo_apellido">
   <column name="SEGUNDO_APELLIDO"/>
  </property>
 </class>
</hibernate-mapping>