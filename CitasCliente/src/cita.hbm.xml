<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.sanmi.citasClientes.hibernate.bean.Cita" table="CITA">
  <id column="ID" name="id" type="int">
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="cliente_id" type="int">
   <column name="CLIENTE_ID"/>
  </property>
  <property generated="never" lazy="false" name="data" type="date">
   <column name="DATA"/>
  </property>
  <property generated="never" lazy="false" name="nota">
   <column name="NOTA"/>
  </property>
 </class>
</hibernate-mapping>
